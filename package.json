{
	"name": "custom-selected-word-count",
	"version": "1.6.2",
	"description": "Comprehensive text analysis for selected text including word counting, character counting, and sentence counting with modern UI design",
	"main": "main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"version": "node version-bump.mjs && git add manifest.json versions.json",
		"lint": "eslint . --ext .ts",
		"watch": "node esbuild.config.mjs dev"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/banisterious/obsidian-custom-selected-word-count"
	},
	"bugs": {
		"url": "https://github.com/banisterious/obsidian-custom-selected-word-count/issues"
	},
	"keywords": [
		"obsidian",
		"plugin",
		"word-count",
		"selected-word-count",
		"text-analysis"
	],
	"author": "John Banister",
	"license": "MIT",
	"devDependencies": {
		"@types/node": "^16.11.6",
		"@typescript-eslint/eslint-plugin": "5.29.0",
		"@typescript-eslint/parser": "5.29.0",
		"builtin-modules": "3.3.0",
		"esbuild": "0.17.3",
		"obsidian": "1.4.11",
		"tslib": "2.4.0",
		"typescript": "4.7.4"
	}
}
